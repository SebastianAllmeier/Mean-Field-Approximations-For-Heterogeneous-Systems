@article{beckElementaryProofConvergence2016,
	abstract = {The susceptible-infected-recovered (SIR) model has been used extensively to model disease spread and other processes. Despite the widespread usage of this ordinary differential equation (ODE) based model which represents the mean-field approximation of the underlying stochastic SIR process on contact networks, only few rigorous approaches exist and these use complex semigroup and martingale techniques to prove that the expected fraction of the susceptible and infected nodes of the stochastic SIR process on a complete graph converges as the number of nodes increases to the solution of the mean-field ODE model. Extending the elementary proof of convergence for the SIS process introduced by Armbruster and Beck (2016) to the SIR process, we show convergence using only a system of three ODEs, simple probabilistic inequalities, and basic ODE theory. Our approach can also be generalized to many other types of compartmental models (e.g., susceptible-infected-recovered-susceptible (SIRS)) which are linear ODEs with the addition of quadratic terms for the number of new infections similar to the SI term in the SIR model.},
	archiveprefix = {arXiv},
	author = {Beck, Ekkehard and Armbruster, Benjamin},
	eprint = {1511.08572},
	eprinttype = {arxiv},
	file = {C\:\\Users\\Basti\\Zotero\\storage\\86899K2Z\\Beck und Armbruster - 2016 - Elementary proof of convergence to the mean-field .pdf},
	journal = {arXiv:1511.08572},
	keywords = {Computer Science - Social and Information Networks,Mathematics - Dynamical Systems,Mathematics - Probability,Quantitative Biology - Populations and Evolution},
	language = {en},
	month = dec,
	title = {Elementary Proof of Convergence to the Mean-Field Model for the {{SIR}} Process},
	year = {2016}}

@article{benaimClassMeanField2008,
	abstract = {We consider models of N interacting objects, where the interaction is via a common resource and the distribution of states of all objects. We introduce the key scaling concept of intensity; informally, the expected number of transitions per object per time slot is of the order of the intensity. We consider the case of vanishing intensity, i.e.~the expected number of object transitions per time slot is o(N). We show that, under mild assumptions and for large N, the occupancy measure converges, in mean square (and thus in probability) over any finite horizon, to a deterministic dynamical system. The mild assumption is essentially that the coefficient of variation of the number of object transitions per time slot remains bounded with N. No independence assumption is needed anywhere. The convergence results allow us to derive properties valid in the stationary regime. We discuss when one can assure that a stationary point of the ODE is the large N limit of the stationary probability distribution of the state of one object for the system with N objects. We use this to develop a critique of the fixed point method sometimes used in conjunction with the decoupling assumption.},
	author = {Bena{\"\i}m, Michel and Le Boudec, Jean-Yves},
	doi = {10.1016/j.peva.2008.03.005},
	file = {C\:\\Users\\Basti\\Zotero\\storage\\QJP5M5ST\\Bena{\"\i}m und Le Boudec - 2008 - A class of mean field interaction models for compu.pdf;C\:\\Users\\Basti\\Zotero\\storage\\GUU73X74\\S0166531608000278.html},
	issn = {0166-5316},
	journal = {Performance Evaluation},
	keywords = {Bianchi's formula,Decoupling assumption,Dynamical system,Fixed point,Game theory,MAC protocol,Markov chain,Mean field interaction model,Reputation systems},
	language = {en},
