@article{albertEstimatingInfinitesimalGenerator1992,
	abstract = {Let \{Z(t),t{$>$}0\} be a separable, continuous time Markov Process with stationary transition probabilities Pij(t),i,j=1,2,{$\cdots$},M. Under suitable regularity conditions, the matrix of transition probabilities, P(t), can be expressed in the form P(t)=exptQ, where Q is an M\texttimes M matrix and is called the "infinitesimal generator" for the process. In this paper, a density on the space of sample functions over [0,t) is constructed. This density depends upon Q. If Q is unknown, the maximum likelihood estimate Q\^(k,t)={$\parallel$}q\^ij(k,t){$\parallel$}, based upon k independent realizations of the process over [0,t) can be derived. If each state has positive probability of being occupied during [0,t) and if the number of independent observations, k, grows larger (t held fixed), then q\^ij is strongly consistent and the joint distribution of the set \{k12(q\^ij-qij)\}i{$\neq$}j (suitably normalized), is asymptotically normal with zero mean and covariance equal to the identity matrix. If k is held fixed (at one, say) and if t grows large, then q\^ij is again strongly consistent and the joint distribution of the set \{t12(q\^ij-qij)\}i{$\neq$}j (suitably normalized), is asymptotically normal with zero mean and covariance equal to the identity matrix, provided that the process \{Z(t),t{$>$}0\} is positively regular. The asymptotic variances of the q\^ij are computed in both cases.},
	author = {Albert, Arthur},
	doi = {doi:10.1214/aoms/1177704594},
	file = {C\:\\Users\\Basti\\Zotero\\storage\\453QPJJW\\euclid.aoms.1177704594.pdf},
	journal = {Ann. Math. Statist., Volume 33, Number 2 (1962), 727-753.},
	title = {Estimating the {{Infinitesimal Generator}} of a {{Continuous Time}}, {{Finite State Markov Process}}},
	year = {1992},
	Bdsk-Url-1 = {https://doi.org/10.1214/aoms/1177704594}}

