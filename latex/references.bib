	language = {en},
	month = jun,
	number = {1},
	pages = {191--202},
	title = {A Mean Field Model for a Class of Garbage Collection Algorithms in Flash-Based Solid State Drives},
	volume = {41},
	year = {2013},
	Bdsk-Url-1 = {https://doi.org/10.1145/2494232.2465543}}

